
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>MSFJS Web Interface</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="MSFJS Web Interface">
    <meta name="author" content="Adam Brady">

    <!-- Le styles -->
    <link href="/css/bootstrap.css" rel="stylesheet">

    <style>

      #noprocesses {

        color: white;
        margin-bottom: 25px;
        text-align: center;

      }

      .breadcrumb {

        text-align: center;

      }

    </style>

    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

  </head>

  <body>

    <!-- Navbar
    ================================================== -->
    <div class="navbar navbar-fixed">
     <div class="navbar-inner">
       <div class="container">
         <a class="brand" href="../">MSFJS</a>
         <div class="nav-collapse" id="main-menu">
          <ul class="nav" id="main-menu-left">
            <li><a id="swatch-link" href="/">Home</a></li>
            <li><a id="swatch-link" href="/exploits">Exploits</a></li>
            <li class="active"><a id="swatch-link" href="/sessions">Sessions</a></li>
            <li><a id="swatch-link" href="/payloads">Payloads</a></li>
            <li><a id="swatch-link" href="/launch">  New Exploit </a></li>
          </ul>
         </div>
       </div>
     </div>
   </div>

   <div class="container">

        <!-- System Info
        ================================================== -->
        <section id="processes">
          <div class="page-header">
            <h1>Session <%=sessionid%></h1>
          </div>

          <ul class="breadcrumb">
            <li class="active"><a href="/sessions/<%=sessionid%>">Session</a> <span class="divider">/</span></li>
            <li><a href="/sessions/<%=sessionid%>/processes">Processes</a> <span class="divider">/</span></li>
            <li><a href="/sessions/<%=sessionid%>/networking">Networking</a></li>
          </ul>

          <div class="span5">

            <legend>PC Info</legend>

            <table class="table table-bordered table-striped table-hover">
              <thead>
                <tr>
                </tr>
              </thead>
              <tbody>

                <tr>
                  <td> Hostname </td>
                  <td> <%=sysdata.name%> </td>
                <tr>

                <tr>
                  <td> OS Version </td>
                  <td> <%=osdata.caption%> <%=osdata.version%> </td>
                </tr>

                <% if(osdata.serialnumber) { %>

                  <tr>
                    <td> OS Serial </td>
                    <td> <%=osdata.serialnumber%> </td>
                  </tr>

                <% } %>

                <tr>
                  <td> Manufacturer </td>
                  <td> <%=sysdata.manufacturer%> </td>
                </tr>

                <tr>
                  <td> RAM </td>
                  <td> <%=Math.floor(sysdata.totalphysicalmemory/1024/1024)%> MB </td>
                </td>

                <tr>
                  <td> CPU Name </td>
                  <td> <%=cpudata.name%> <b><i>X</i></b> <%=cpudata.numberofcores%> </td>
                </tr>

                <tr>
                  <td> CPU Manufacturer </td>
                  <td> <%=cpudata.manufacturer%> </td>
                </tr>

              </tbody>
            </table>

          </div>

          <div class="span5 pull-right">


            <legend>Session Info</legend>

            <table class="table table-bordered table-striped table-hover">
              <thead>
                <tr>
                </tr>
              </thead>
              <tbody>

                <tr>
                  <td> Process ID </td>
                  <td> <%=pid%> <a href="/sessions/<%=sessionid%>/kill" class="btn btn-danger pull-right">Kill Session</a> </td>
                </tr>

                <tr>
                  <td> Exploit Used </td>
                  <td> <%=session.access.exploit%> </td>
                </tr>

                <tr>
                  <td> Commands in queue </td>
                  <td> <%=session.buffer.length%> </td>
                </tr>

                <tr>
                  <td> Ports </td>
                  <td>
                      Local: <%=session.connection.from.port%> <br />
                      Remote: <%=session.connection.to.port%>
                  </td>
                </tr>


              </tbody>
            </table>

          </div>

          <legend>Password Hashes</legend>

            <table class="table table-bordered table-striped table-hover">
              <thead>
                <tr>
                  <th>#</th>
                  <th>User</th>
                  <th>ID</th>
                  <th>LM</th>
                  <th>NTLM</th>
                </tr>
              </thead>
              <tbody>

                <% for(var i = 0; i < hashes.length; i++) { %>

                  <tr>
                    <td> <%=i%> </td>
                    <td> <%=hashes[i].user%> </td>
                    <td> <%=hashes[i].id%> </td>
                    <td> <%=hashes[i].lm%> </td>
                    <td> <%=hashes[i].ntlm%> </td>
                  <tr>

                <% } %>

              </tbody>
            </table>

        </section>

    </div>

    <!-- Footer
    ================================================== -->
    <footer class="footer">

      <center>
        <a href="http://github.com/SomeoneWeird/msfjsweb/">MSFJS on GitHub</a>
      </center>

    </footer>
  </body>
</html>
